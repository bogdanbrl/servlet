<?xml version="1.0" encoding="UTF-8"?>
<web-app 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  <welcome-file-list>
    <welcome-file>Servlet_demonstration</welcome-file>
  </welcome-file-list>
   
  <servlet>
    <servlet-name>Servlet_demonstration</servlet-name>
    <servlet-class>function_tags.Servlet_demonstration</servlet-class>
  </servlet>
   
  <servlet-mapping>
    <servlet-name>Servlet_demonstration</servlet-name>
    <url-pattern>/Servlet_demonstration</url-pattern>
  </servlet-mapping>
  
  
</web-app>